{
    "name": "@fluent-plus/workspace-plugin",
    "version": "0.0.1",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "@fluent-plus/fluent-plus-repo": "./src/index.ts",
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        }
    },
    "nx": {
        "targets": {
            "build": {
                "executor": "@nx/js:tsc",
                "outputs": [
                    "{options.outputPath}"
                ],
                "options": {
                    "outputPath": "tools/workspace-plugin/dist",
                    "main": "tools/workspace-plugin/src/index.ts",
                    "tsConfig": "tools/workspace-plugin/tsconfig.lib.json",
                    "rootDir": "tools/workspace-plugin/src",
                    "generatePackageJson": false,
                    "assets": [
                        {
                            "input": "./tools/workspace-plugin/src",
                            "glob": "**/!(*.ts)",
                            "output": "."
                        },
                        {
                            "input": "./tools/workspace-plugin/src",
                            "glob": "**/*.d.ts",
                            "output": "."
                        }
                    ]
                }
            }
        },
        "tags": [
            "private"
        ]
    },
    "dependencies": {
        "@nx/devkit": "21.4.1",
        "tslib": "^2.3.0",
        "@nx/react": "^21.4.1",
        "nx": "21.4.1"
    },
    "generators": "./generators.json",
    "files": [
        "dist",
        "!**/*.tsbuildinfo",
        "generators.json"
    ]
}
