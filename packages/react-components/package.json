{
    "name": "@fluent-plus/react-components",
    "version": "0.0.2",
    "type": "module",
    "description": "A collection of React components for building Fluent UI experiences.",
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/mathis-m/fluent-plus"
    },
    "homepage": "https://mathis-m.github.io/fluent-plus",
    "license": "UNLICENSED",
    "main": "./dist/index.esm.js",
    "module": "./dist/index.esm.js",
    "types": "./dist/index.esm.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/index.esm.d.ts",
            "import": "./dist/index.esm.js",
            "default": "./dist/index.esm.js"
        }
    },
    "files": [
        "dist",
        "!**/*.tsbuildinfo"
    ],
    "nx": {
        "targets": {
            "test": {
                "executor": "@nx/jest:jest",
                "outputs": [
                    "{projectRoot}/test-output/jest/coverage"
                ],
                "options": {
                    "jestConfig": "packages/react-components/jest.config.ts"
                }
            }
        }
    },
    "peerDependencies": {
        "@types/react": ">=16.14.0 <20.0.0",
        "@types/react-dom": ">=16.9.0 <20.0.0",
        "react": ">=16.14.0 <20.0.0",
        "react-dom": ">=16.14.0 <20.0.0",
        "@fluentui/react-components": ">=9.69.0 <10.0.0",
        "@fluentui/react-context-selector": ">=9.2.6 <10.0.0",
        "@fluentui/react-utilities": ">=9.24.0 <10.0.0"
    },
    "dependencies": {
        "@fluent-plus/file-upload": "^0.0.2"
    },
    "publishConfig": {
        "access": "public"
    }
}
