{
    "name": "@fluent-plus/public-docsite",
    "version": "0.0.1",
    "nx": {
        "name": "public-docsite",
        "targets": {
            "build-storybook": {
                "dependsOn": [{
                    "target": "build",
                    "dependencies": true
                }]
            },
            "test": {
                "executor": "@nx/jest:jest",
                "outputs": [
                    "{projectRoot}/test-output/jest/coverage"
                ],
                "options": {
                    "jestConfig": "apps/public-docsite/jest.config.ts"
                }
            }
        },
        "implicitDependencies": ["tag:type:stories"]
    }
}
