{
    "extends": "../../tsconfig.base.json",
    "compilerOptions": {
        "outDir": "out-tsc/storybook",
        "module": "esnext",
        "moduleResolution": "bundler",
        "jsx": "react-jsx",
        "lib": ["dom"]
    },
    "exclude": [
        "src/**/*.spec.ts",
        "src/**/*.test.ts",
        "src/**/*.spec.js",
        "src/**/*.test.js",
        "src/**/*.spec.tsx",
        "src/**/*.test.tsx",
        "src/**/*.spec.jsx",
        "src/**/*.test.js"
    ],
    "include": [
        "src/**/*.stories.ts",
        "src/**/*.stories.js",
        "src/**/*.stories.jsx",
        "src/**/*.stories.tsx",
        "src/**/*.stories.mdx",
        ".storybook/*.js",
        ".storybook/*.ts",
        "../../packages/**/*.stories.ts",
        "../../packages/**/*.stories.js",
        "../../packages/**/*.stories.jsx",
        "../../packages/**/*.stories.tsx",
        "../../packages/**/*.stories.mdx",
        "../../packages/**/*.tsx",
        "../../packages/**/*.ts",
        "../../packages/**/*.mdx"
    ],
    "references": [
        {
            "path": "./tsconfig.lib.json"
        }
    ],
    "files": [
        "../../node_modules/@nx/react/typings/styled-jsx.d.ts",
        "../../node_modules/@nx/react/typings/cssmodule.d.ts",
        "../../node_modules/@nx/react/typings/image.d.ts"
    ]
}
